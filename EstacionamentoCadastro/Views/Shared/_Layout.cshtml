<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - EstacionamentoCadastro</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/Content/light-theme.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/toastr.js/toastr.css" rel="stylesheet" />
    <link href="~/assets/plugins/bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/bootstrap-datetimepicker.css" rel="stylesheet" />
    <link href="~/assets/plugins/bootstrap-calendar/css/bootstrap_calendar.css" rel="stylesheet" />
    <link href="~/assets/plugins/font-awesome/css/fontawesome.css" rel="stylesheet" />
    <link href="~/assets/css/animate.css" rel="stylesheet" />
    <link href="~/assets/css/style.css" rel="stylesheet" />
    <link href="~/assets/css/style-responsive.css" rel="stylesheet" />


</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" style="width: 258px;margin-left: -30px;" asp-area="" asp-controller="Home" asp-action="Index">Estacionamento Cadastro</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Carro" asp-action="Index">Carro</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Manobrista" asp-action="Index">Manobrista</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="CarroManobrado" asp-action="Index">Carros Manobrados</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - EstacionamentoCadastro - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/Scripts/jquery-1.12.0.min.js"></script>
    <script src="~/Scripts/jquery.validate.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/Scripts/bootstrap-datetimepicker.min.js"></script>
    <script src="~/Scripts/global.js"></script>
    <script src="~/Scripts/_toastr/toastr.js"></script>
    <script src="~/Scripts/bootbox.js"></script>


    <script src="~/assets/plugins/jquery-ui/ui/jquery-ui.js"></script>
    <script src="~/assets/plugins/bootstrap/js/bootstrap.js"></script>
    <script src="~/assets/plugins/slimscroll/jquery.slimscroll.js"></script>
    <script src="~/assets/plugins/jquery-cookie/jquery.cookie.js"></script>
    <script src="~/assets/plugins/flot/jquery.flot.js"></script>
    <script src="~/assets/plugins/flot/jquery.flot.time.js"></script>
    <script src="~/assets/plugins/flot/jquery.flot.resize.js"></script>
    <script src="~/assets/plugins/flot/jquery.flot.pie.js"></script>
    <script src="~/Scripts/moment.js"></script>
    <script src="~/assets/plugins/jquery-tag-it/js/tag-it.js"></script>
    <script src="~/assets/plugins/bootstrap-calendar/js/bootstrap_calendar.js"></script>
    <script src="~/assets/plugins/bootstrap-multiselect/js/bootstrap-multiselect.js"></script>
    <script src="~/assets/plugins/bootstrap-confirmation/bootstrap-confirmation.min.js"></script>
    <script src="~/assets/plugins/bootstrap-switch/js/bootstrap-switch.min.js"></script>
    <script src="~/assets/plugins/morris/js/morris.min.js"></script>
    <script src="~/assets/plugins/morris/js/raphael.min.js"></script>
    <script src="~/assets/plugins/bootstrap-colorpicker-master/dist/js/bootstrap-colorpicker.min.js"></script>
    <script src="~/assets/js/apps.js"></script>

    <script src="~/Scripts/bootbox.js"></script>
    <script src="~/assets/plugins/select2/js/select2.min.js"></script>

    <script src="~/Scripts/jquery.mask.js"></script>
    <script src="~/Scripts/jquery.maskMoney.min.js"></script>
    <script src="~/Scripts/ConfigMask.js"></script>


    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/plugins/bootstrap-calendar/js/bootstrap_calendar.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/Content/toastr.js/toastr.min.js"></script>

    @await Component.InvokeAsync("NToastNotify")
    @RenderSection("scripts", required: false)
    <script>
        $(document).ready(function () {
            if (localStorage["sideMenu"] === 'closed') {
                $('#page-container').addClass('page-sidebar-minified');
            }
            if (localStorage["theme"] === 'light') {
                $('#lightTheme').prop('disabled', false);
            }

            $('#btnTrocaTema').on('click', function () {
                var disabled = $("#lightTheme").prop('disabled');
                if (disabled) {
                    localStorage["theme"] = 'light';
                    $('#lightTheme').prop('disabled', false);
                } else {
                    localStorage["theme"] = 'dark';
                    $('#lightTheme').prop('disabled', true);
                }
                if (typeof CarregarPainelPercentualGrafico == 'function')
                    CarregarPainelPercentualGrafico();
            });
            App.init();
            //Dashboard.init();

            $("#menu li.sub-active").removeClass('sub-active').parent().closest('li').addClass('expand active');
        });


        toastr.options = {
            "closeButton": true,
            "debug": false,
            "newestOnTop": false,
            "progressBar": true,
            "positionClass": "toast-top-right",
            "preventDuplicates": true,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
    </script>
</body>
</html >
